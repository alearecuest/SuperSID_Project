{
  "name": "supersid-pro-analytics",
  "version": "1.0.0",
  "description": "Professional SuperSID Data Acquisition and Analysis Platform with AR visualization and advanced analytics",
  "author": "Alejandro ArÃ©valo",
  "license": "MIT",
  "type": "module",
  "main": "dist/main/index.js",
  "homepage": "./",
  "scripts": {
    "dev:backend": "nodemon --watch src/backend --ext ts --exec \"npm run build:backend && node dist/backend/server.js\"",
    "build:backend": "tsc --project tsconfig.backend.json --skipLibCheck",
    "start:backend": "node dist/backend/server.js",
    "dev:renderer": "webpack serve --mode development --config webpack.config.cjs",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:renderer\"",
    "build": "npm run build:backend && npm run build:renderer",
    "build:renderer": "webpack --mode production --config webpack.config.cjs",
    "start": "node dist/backend/server.js",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run build\" \"electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "eslint src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.6.0",
    "chart.js": "^4.5.1",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "fft.js": "^4.0.4",
    "lodash": "^4.17.21",
    "node-record-lpcm16": "^1.0.1",
    "plotly.js": "^2.26.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.4",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "sox-audio": "^0.3.0",
    "sqlite3": "^5.1.7",
    "ssh2-sftp-client": "^12.0.1",
    "three": "^0.154.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.14",
